  keywords = {fMRI, Functional neuroimaging, Hype, Neuroskepticism, Skepticism},
  pages = {77--81},
  file = {Rachul_Zarzeczny_2012_The rise of neuroskepticism.pdf:/home/asura/Zotero/storage/VUJVD98H/Rachul_Zarzeczny_2012_The rise of neuroskepticism.pdf:application/pdf;ScienceDirect Snapshot:/home/asura/Zotero/storage/JC438N7S/S0160252711001567.html:text/html},
